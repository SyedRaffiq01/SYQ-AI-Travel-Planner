{
  "build": {
    "builder": "DOCKERFILE"
  },
  "deploy": {
    "startCommand": "cd 'Travel Planner agent/Travel-Planner-AI' && uvicorn app:app --host 0.0.0.0 --port $PORT",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}